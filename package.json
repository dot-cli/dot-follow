{
  "name": "dot-follow",
  "version": "0.0.0",
  "description": "Follow developers with ease",
  "author": "chico @chico",
  "bin": {
    "dot-follow": "./bin/run"
  },
  "homepage": "https://github.com/dot-cli/dot-follow",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "dot-cli/dot-follow",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.0",
    "@octokit/graphql": "^5.0.1",
    "application-config": "^2.0.0",
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "ghauth": "chico/ghauth.git#5.0.2",
    "github-api": "^3.4.0",
    "inquirer": "^8.2.0",
    "open": "^8.4.0",
    "remarkable": "^2.0.1",
    "slack-webhook": "^1.0.0",
    "social-media-scraper": "^0.0.7"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@oclif/test": "^2.1.0",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/chai": "^4",
    "@types/inquirer": "^8.2.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.36",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "globby": "^11.1.0",
    "madge": "^5.0.1",
    "mocha": "^9",
    "nyc": "^15.1.0",
    "oclif": "^3",
    "prettier": "^2.6.2",
    "shx": "^0.3.4",
    "sinon": "^14.0.0",
    "ts-node": "^10.8.0",
    "tslib": "^2.3.1",
    "typescript": "^4.7.2"
  },
  "oclif": {
    "bin": "dot-follow",
    "dirname": "dot-follow",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "build:watch": "yarn build && tsc -w",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint && ./node_modules/.bin/madge --circular src",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "NODE_ENV=test nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "test-single": "NODE_ENV=test mocha",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/dot-cli/dot-follow/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
